<% provide(:title, "Edit Transaction") %>
<h1>Edit Transaction</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for [@account, @transaction] do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
